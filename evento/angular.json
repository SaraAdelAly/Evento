{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "evento": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/evento",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "src/assets/css/style.css",
              "src/assets/css/slick.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/magnific-popup.css",
              "node_modules/@mobiscroll/angular/dist/css/mobiscroll.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/primeng/resources/primeng.min.css",
              "src/assets/css/style.css",
              "src/assets/admin/plugins/pg-calendar/css/pignose.calendar.min.css" ,
              "src/assets/admin/plugins/chartist/css/chartist.min.css",
              "src/assets/admin/plugins/chartist-plugin-tooltips/css/chartist-plugin-tooltip.css",
              "src/assets/admin/plugins/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css",
              "src/assets/admin/plugins/jquery-asColorPicker-master/css/asColorPicker.css",
              "src/assets/admin/plugins/bootstrap-datepicker/bootstrap-datepicker.min.css",
              "src/assets/admin/plugins/timepicker/bootstrap-timepicker.min.css",
              "src/assets/admin/plugins/bootstrap-daterangepicker/daterangepicker.css",
              "src/assets/css/admin/style.css"

            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/script.js",
              "src/assets/js/custom.min.js",
              "src/assets/js/settings.js",
              "src/assets/js/gleek.js",
              "src/assets/js/styleSwitcher.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery.syotimer.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/shuffle.min.js",
              "src/assets/js/gmap.js",
              "src/assets/admin/plugins/chart.js/Chart.bundle.min.js",
              "src/assets/admin/plugins/common/common.min.js",
              "src/assets/admin/plugins/circle-progress/circle-progress.min.js",
              "src/assets/admin/plugins/d3v3/index.js",
              "src/assets/admin/plugins/topojson/topojson.min.js",
              "src/assets/admin/plugins/datamaps/datamaps.world.min.js",
              "src/assets/admin/plugins/raphael/raphael.min.js",
              "src/assets/admin/plugins/morris/morris.min.js",
              "src/assets/admin/plugins/moment/moment.min.js",
              "src/assets/admin/plugins/pg-calendar/js/pignose.calendar.min.js",
              "src/assets/admin/plugins/chartist/js/chartist.min.js",
              "src/assets/admin/plugins/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js",
              "src/assets/admin/plugins/validation/jquery.validate.min.js",
              "src/assets/admin/plugins/validation/jquery.validate-init.js",
              "src/assets/js/dashboard/dashboard-1.js",
              "src/assets/admin/plugins/moment/moment.js",
              "src/assets/admin/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js",
              "src/assets/admin/plugins/clockpicker/dist/jquery-clockpicker.min.js",
              "src/assets/admin/plugins/jquery-asColorPicker-master/libs/jquery-asColor.js",
              "src/assets/admin/plugins/jquery-asColorPicker-master/libs/jquery-asGradient.js",
              "src/assets/admin/plugins/jquery-asColorPicker-master/dist/jquery-asColorPicker.min.js",
              "src/assets/admin/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js",
              "src/assets/admin/plugins/timepicker/bootstrap-timepicker.min.js",
              "src/assets/admin/plugins/bootstrap-daterangepicker/daterangepicker.js",
              "src/assets/admin/plugins/timepicker/bootstrap-timepicker.min.js",
              "src/assets/admin/plugins/bootstrap-daterangepicker/daterangepicker.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "evento:build:production"
            },
            "development": {
              "browserTarget": "evento:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "evento:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/scrip.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "1b81bc0c-a65b-4e34-94a6-92bc37e48df5"
  }
}
